{
    "manifest_version": 2,
    "name": "AI Hotissue Collector",
    "version": "1.0.0",
    "default_locale": "ko",
    "description": "This app is Crawling Hot Community Posts",
    "icons": {
        "128": "./static/noun_canyon_3005142.png"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}